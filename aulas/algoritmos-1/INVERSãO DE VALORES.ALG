Algoritmo "Troca"

Var
    X, Y, Troca1, Troca2: inteiro;


Inicio
   X<- 5; {Processamento de dados}
   Y<-10 ; {Processamento de dados}
   Troca1<- Y; {Processamento de dados}
   Troca2<- X ;{Processamento de dados}
   Escreval("O valor de X passará a ser: ",Troca1); {Saída de dados}
   Escreva("O valor de Y passará a ser: ", Troca2);{Saída de dados}
   
Fimalgoritmo